/* To find whether the given number is odd or even */


#include <stdio.h>
#include <stdlib.h>

int main()
{
  int number;
  printf("Enter the integer\n",number);
  scanf("%d",&number);

  if (number%2==0)
  {
      printf("Number is even\n");
  }
  else
    printf("Number is odd");

    return 0;
}
